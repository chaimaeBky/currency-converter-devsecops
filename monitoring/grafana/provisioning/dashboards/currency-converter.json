{
  "dashboard": {
    "title": "Currency Converter Metrics",
    "panels": [
      {
        "title": "HTTP Requests",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}"
          }
        ]
      },
      {
        "title": "CPU Usage",
        "type": "stat",
        "targets": [
          {
            "expr": "process_cpu_seconds_total"
          }
        ]
      }
    ]
  }
}
